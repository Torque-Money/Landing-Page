---
import config from "../../../../astro.config.mjs";

// @ts-ignore
const {misc} = config;
---

<script async src={`https://www.googletagmanager.com/gtag/js?id=${misc.gaId}`}></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag() {
        dataLayer.push(arguments);
    }
    gtag("js", new Date());

    gtag("config", misc.gaId);
</script>
