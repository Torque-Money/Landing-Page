---
import config from "../../../../astro.config.mjs";

const {misc} = config as any;
const gaURL = "https://www.googletagmanager.com/gtag/js?id=" + misc.gaId;
---

<script async src={gaURL}></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag() {
        dataLayer.push(arguments);
    }
    gtag("js", new Date());

    gtag("config", misc.gaId);
</script>
